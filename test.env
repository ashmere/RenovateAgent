# GitHub Authentication (for production GitHub App mode)
GITHUB_APP_ID=123456
GITHUB_APP_PRIVATE_KEY_PATH=/tmp/test-key.pem
GITHUB_WEBHOOK_SECRET=test-secret-key
GITHUB_ORGANIZATION=test-org

# Operation Mode Configuration (DEFAULT: Polling enabled for local testing)
ENABLE_POLLING=true
ENABLE_WEBHOOKS=false
POLLING_INTERVAL_MINUTES=2
POLLING_MAX_CONCURRENT_REPOS=5

# Repository Configuration
GITHUB_TEST_REPOSITORIES=test-org/test-repo1,test-org/test-repo2
POLLING_REPOSITORIES=test-org/test-repo1,test-org/test-repo2

# Polling Rate Limiting
GITHUB_API_RATE_LIMIT=5000
POLLING_RATE_LIMIT_BUFFER=1000
POLLING_RATE_LIMIT_THRESHOLD=0.8

# Dependency Fixing Configuration
ENABLE_DEPENDENCY_FIXING=true
SUPPORTED_LANGUAGES=python,typescript,go

# Server Configuration
DEBUG=true
LOG_LEVEL=DEBUG
HOST=127.0.0.1
PORT=8001
